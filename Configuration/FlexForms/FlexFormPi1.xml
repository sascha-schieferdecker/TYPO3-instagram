<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<main>
			<ROOT>
				<TCEforms>
					<sheetTitle>Instagram</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.username>
						<TCEforms>
							<exclude>1</exclude>
							<label>username</label>
							<config>
								<type>input</type>
								<placeholder>username</placeholder>
								<eval>trim,required</eval>
							</config>
						</TCEforms>
					</settings.username>
					<settings.limit>
						<TCEforms>
							<exclude>1</exclude>
							<label>Limit</label>
							<config>
								<type>input</type>
								<default>10</default>
								<eval>int,required</eval>
							</config>
						</TCEforms>
					</settings.limit>
				</el>
			</ROOT>
		</main>
	</sheets>
</T3DataStructure>
