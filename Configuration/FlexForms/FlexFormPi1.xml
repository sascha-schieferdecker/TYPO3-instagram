<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<main>
			<ROOT>
				<TCEforms>
					<sheetTitle>Instagram</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.usernames>
						<TCEforms>
							<label>Show posts from these Instagram accounts (one per line):</label>
							<config>
								<type>text</type>
								<cols>30</cols>
								<rows>5</rows>
								<eval>trim,required</eval>
							</config>
						</TCEforms>
					</settings.usernames>
					<settings.limit>
						<TCEforms>
							<exclude>1</exclude>
							<label>Limit</label>
							<config>
								<type>input</type>
								<default>10</default>
								<eval>trim,int,required</eval>
								<range>
									<lower>1</lower>
									<upper>100</upper>
								</range>
								<slider>
									<step>1</step>
									<width>400</width>
								</slider>
							</config>
						</TCEforms>
					</settings.limit>
				</el>
			</ROOT>
		</main>
	</sheets>
</T3DataStructure>
